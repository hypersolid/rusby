use std::io::{self, Write};
use std::ffi::{CStr,CString};
use std::os::raw::c_char;

// this function folds ffi arguments and unfolds result to ffi types
#[no_mangle]
pub extern "C"
 fn ffi_plusplus(number: i32) -> i32 {
let result = plusplus(number);
return result;
}

#[allow(unused_mut)]
fn plusplus(number: i32) -> i32 {
number+1
}